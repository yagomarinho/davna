{
  "name": "@davna/api",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "rimraf ./dist && npm run typecheck && esbuild src/index.ts --bundle --platform=node --outdir=dist --minify",
    "dev": "tsnd --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "start": "node ./dist/index.js",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --testPathPatterns=\"src/modules/.*/__tests__/.*.spec.ts$\"",
    "typecheck": "tsc --noEmit --project ./tsconfig.build.json"
  },
  "author": {
    "name": "Yago Marinho",
    "email": "marinho.rj2@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yagomarinho/davna.git"
  },
  "dependencies": {
    "@davna/applications": "*",
    "dotenv": "^17.2.2",
    "multer": "^2.0.2",
    "music-metadata": "^11.10.2",
    "socket.io": "^4.8.1",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4"
  }
}

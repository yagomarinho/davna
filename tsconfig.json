{
  "compilerOptions": {
    "target": "ES2017",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "module": "nodenext",
    "moduleResolution": "nodenext"
  },
  "include": [
    "apps",
    "config",
    "modules/lead",
    "modules/classroom",
    "modules/account"
  ],
  "exclude": ["node_modules", "coverage"]
}
